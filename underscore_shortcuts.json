{
    "title": "Underscore Shortcuts : Works in JIS keyboard",
    "rules": [
        {
            "description": "Underscore + E to open Microsoft Edge",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "international1",
                        "modifiers": {
                            "mandatory": [],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "set_variable": {
                                "name": "custom_modifier",
                                "value": 1
                            }
                        }
                    ],
                    "to_after_key_up": [
                        {
                            "set_variable": {
                                "name": "custom_modifier",
                                "value": 0
                            }
                        }
                    ],
                    "to_if_alone": [
                        {
                            "key_code": "international1"
                        }
                    ]
                },
                {
                    "type": "basic",
                    "from": {
                        "key_code": "e",
                        "modifiers": {
                            "mandatory": []
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open -a '/Applications/Microsoft Edge.app'"
                        }
                    ],
                    "conditions": [
                        {
                            "type": "variable_if",
                            "name": "custom_modifier",
                            "value": 1
                        }
                    ]
                }
            ]
        },
