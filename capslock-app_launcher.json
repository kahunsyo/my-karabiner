{
    "title": "CapsLock App Launcher",
    "rules": [
        {
            "description": "CapsLock + W to launch Microsoft Edge",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "w",
                        "modifiers": {
                            "mandatory": [
                                "caps_lock"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open -a '/Users/sora/Applications/new_window_edge.app'"
                        }
                    ]
                }
            ]
        },
        {
            "description": "CapsLock + E to launch Finder",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "key_code": "e",
                        "modifiers": {
                            "mandatory": [
                                "caps_lock"
                            ],
                            "optional": [
                                "any"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open -a '/Users/sora/Applications/new_window_finder.app'"
                        }
                    ]
                }
            ]
        }
    ]
}